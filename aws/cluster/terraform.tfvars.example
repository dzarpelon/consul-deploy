# Example terraform.tfvars for Consul AWS Cluster
region = "eu-central-1"
instance_count = 3
instance_type = "t3.micro"
disk_size = 10
disk_type = "gp3"
public_ip = true
key_name = "your-ssh-key-name"
ssh_cidr = ["0.0.0.0/0"]
instance_names = ["consul-1", "consul-2", "consul-3"]
tags = {
  Environment = "lab"
  Project     = "consul-lab"
}
